{
    "name": "minecraft-map-installer",
    "version": "1.0.0",
    "description": "A desktop app to easily add new Minecraft maps automatically",
    "repository": "gitsomething",
    "author": "Luis Felipe (@luisfelipesdn12)",
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "lint": "eslint -c .eslintrc.yml --ext .ts ./src",
        "lint:fix": "eslint -c .eslintrc.yml --ext .ts ./src --fix",
        "start": "npm run build && electron ./dist/main.js"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^4.21.0",
        "@typescript-eslint/parser": "^4.21.0",
        "electron": "^9.4.0",
        "eslint": "^7.3.1",
        "typescript": "^3.9.5"
    },
    "dependencies": {
        "minecraft-path": "^1.0.0",
        "node-stream-zip": "^1.13.3"
    }
}
